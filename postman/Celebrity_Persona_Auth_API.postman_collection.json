  "item": [
    {
      "name": "SuperAdmin APIs",
      "item": [
        {
          "name": "Create SuperAdmin (Special Key Required)",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"email\": \"superadmin@celebritypersona.com\",\n  \"password\": \"SuperAdmin123!\",\n  \"specialKey\": \"{{specialKey}}\"\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/api/auth/superadmin/create",
              "host": ["{{baseUrl}}"],
              "path": ["api", "auth", "superadmin", "create"]
            },
            "description": "Create a SuperAdmin account. Only one SuperAdmin can exist. Requires special key."
          }
        },
        {
          "name": "SuperAdmin Login",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"email\": \"superadmin@celebritypersona.com\",\n  \"password\": \"SuperAdmin123!\"\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/api/auth/superadmin/login",
              "host": ["{{baseUrl}}"],
              "path": ["api", "auth", "superadmin", "login"]
            },
            "description": "SuperAdmin login. Returns access and refresh tokens."
          },
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "if (pm.response.code === 200) {",
                  "    const response = pm.response.json();",
                  "    if (response.data) {",
                  "        pm.environment.set('accessToken', response.data.accessToken);",
                  "        pm.environment.set('refreshToken', response.data.refreshToken);",
                  "    }",
                  "}"
                ],
                "type": "text/javascript"
              }
            }
          ]
        },
        {
          "name": "Get SuperAdmin Profile",
          "request": {
            "method": "GET",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{accessToken}}"
              }
            ],
            "url": {
              "raw": "{{baseUrl}}/api/auth/superadmin/profile",
              "host": ["{{baseUrl}}"],
              "path": ["api", "auth", "superadmin", "profile"]
            },
            "description": "Get SuperAdmin profile information. Requires SuperAdmin access token."
          }
        },
        {
          "name": "Update SuperAdmin Password",
          "request": {
            "method": "PUT",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              },
              {
                "key": "Authorization",
                "value": "Bearer {{accessToken}}"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"currentPassword\": \"SuperAdmin123!\",\n  \"newPassword\": \"NewSuperAdmin123!\"\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/api/auth/superadmin/update-password",
              "host": ["{{baseUrl}}"],
              "path": ["api", "auth", "superadmin", "update-password"]
            },
            "description": "Update SuperAdmin password. Requires current password and SuperAdmin access token."
          }
        },
        {
          "name": "SuperAdmin Reset Password - Request",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"email\": \"superadmin@celebritypersona.com\"\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/api/auth/superadmin/reset-password",
              "host": ["{{baseUrl}}"],
              "path": ["api", "auth", "superadmin", "reset-password"]
            },
            "description": "Request password reset for SuperAdmin. Returns reset token (in production, this should be sent via email)."
          },
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "if (pm.response.code === 200) {",
                  "    const response = pm.response.json();",
                  "    if (response.resetToken) {",
                  "        pm.environment.set('resetToken', response.resetToken);",
                  "    }",
                  "}"
                ],
                "type": "text/javascript"
              }
            }
